/* Usage: course section exam_length num_forms */
int main(int argc, char **argv) {
  char cwd[1024];
  getcwd(cwd, sizeof(cwd));
  if (getcwd(cwd, sizeof(cwd)) != NULL) {
       printf("Current working dir: %s\n", cwd);
   } else {
       perror("getcwd() error");
   }
}

